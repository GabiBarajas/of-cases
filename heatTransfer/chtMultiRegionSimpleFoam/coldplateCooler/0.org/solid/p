FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/domain1";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 100000;

boundaryField
{
    baffleSolidToFluid
    {
        type            calculated;
        value           uniform 100000;
    }
    "wall.*"
    {
        type            calculated;
        value           uniform 100000;
    }
    wallHeatSource
    {
        type            calculated;
        value           uniform 100000;
    }
}


