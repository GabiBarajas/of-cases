FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/domain1";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    baffleSolidToFluid
    {
        type            zeroGradient;
    }
    wallSolidSide
    {
        type            zeroGradient;
    }
    wallSolidBottom
    {
        type            zeroGradient;
    }
}


