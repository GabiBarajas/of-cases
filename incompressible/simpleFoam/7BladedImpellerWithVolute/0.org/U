FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            flowRateInletVelocity;
	    volumetricFlowRate table
        (
            ( 0	      0.0002 ) 
            ( 200     0.0002 )
            ( 201     0.0004 )
            ( 400     0.0004 )
            ( 401     0.0006 )
            ( 600     0.0006 )
            ( 601     0.0008 )
            ( 800     0.0008 )
            ( 801     0.001  ) 
            ( 1000    0.001  ) 
            ( 1001    0.0012 )
            ( 1200    0.0012 )
            ( 1201    0.0014 )
        );
        value           uniform (0 0 -1);
    }

    outlet
    {
        type            pressureInletOutletVelocity;
        value           uniform (1 0 0);
    }

    walls
    {
	    type            noSlip;
    }
    
    rotatingWall
    {
	    type            noSlip;
    }

    "ami.*"
    {
        type            cyclicAMI;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}

// ************************************************************************* //
