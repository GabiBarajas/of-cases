FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 10;

boundaryField
{
    inlet
    {
	type            fixedValue;
        value           uniform 10;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    "wall.*"
    {
        type            epsilonWallFunction;
        value           $internalField;
    }

    "slip.*"
    {
        type		slip;
    }

    frontAndBack
    {
        type            empty;
    }
}


// ************************************************************************* //
